	"Upload File Html": {
		"prefix": "upload",
		"body": [
			"<form id=\"${1:(action_name)}Form\" method=\"POST\" action=\"/${1:(action_name)}\" enctype='multipart/form-data'>",
			"\t<input type=\"text\" class=\"form-control\" name=\"${2:(name)}\" />",
			"\t<input type=\"file\" class=\"form-control\" name=\"${3:(file_name)}\" />",
			"\t<button class=\"btn btn-primary mr-5\" type=\"submit\">Submit File</button>",
			"</form>",
			"",
			"<script type=\"text/javascript\">",
			"\tlet form = document.querySelector(\"form[id='${1:(action_name)}Form']\");",
			"\tlet action = form.getAttribute(\"action\");",
			"\tlet method = form.getAttribute(\"method\");",
			"",
			"\tform.addEventListener(\"submit\", e => {",
			"\t\te.preventDefault();",
			"\t\tlet file = form.querySelector(\"input[name='${3:(file_name)}']\").files[0];",
			"\t\tlet name = form.querySelector(\"input[name='${2:(name)}']\").value;",
			"",
			"\t\tlet formData = new FormData();",
			"\t\tformData.append(\"${3:(file_name)}\", file);",
			"\t\tformData.append(\"${2:(name)}\", name);",
			"",
			"\t\tconst fetchData = async () => {",
			"",
			"\t\t\tconst resp = await fetch(action, {",
			"\t\t\t\tmethod: method,",
			"\t\t\t\tbody: formData",
			"\t\t\t});",
			"",
			"\t\t\tconst data = await resp.json();",
			"",
			"\t\t\tconst { ResponseCode } = data;",
			"",
			"\t\t\tif (ResponseCode == \"00\") { }",
			"\t\t}",
			"",
			"\t\tfetchData().catch((err) => {",
			"\t\t\tconsole.error(err);",
			"\t\t});",
			"\t})",
			"</script>"
		],
		"description": "Upload Files"
	},
